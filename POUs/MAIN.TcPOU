    <Action Name="A_GetTemperatureSensorsData" Id="{d73fb87c-9e06-4b36-8cf6-68d5032eab30}">
      <Implementation>
        <ST><![CDATA[//sensor_1
dut_sensor_datas_1.data_byte[3]:=Sensor_Reading_1[0];
dut_sensor_datas_1.data_byte[2]:=Sensor_Reading_1[1];
dut_sensor_datas_1.data_byte[1]:=Sensor_Reading_1[2];
dut_sensor_datas_1.data_byte[0]:=Sensor_Reading_1[3];
TemperatureSensors_1:=DINT_TO_LREAL(LREAL_TO_DINT(dut_sensor_datas_1.data_lreal*100))/100;
//sensor_2
dut_sensor_datas_2.data_byte[3]:=Sensor_Reading_2[0];
dut_sensor_datas_2.data_byte[2]:=Sensor_Reading_2[1];
dut_sensor_datas_2.data_byte[1]:=Sensor_Reading_2[2];
dut_sensor_datas_2.data_byte[0]:=Sensor_Reading_2[3];
TemperatureSensors_2:=DINT_TO_LREAL(LREAL_TO_DINT(dut_sensor_datas_2.data_lreal*100))/100;
//sensor_3
dut_sensor_datas_3.data_byte[3]:=Sensor_Reading_3[0];
dut_sensor_datas_3.data_byte[2]:=Sensor_Reading_3[1];
dut_sensor_datas_3.data_byte[1]:=Sensor_Reading_3[2];
dut_sensor_datas_3.data_byte[0]:=Sensor_Reading_3[3];
TemperatureSensors_3:=DINT_TO_LREAL(LREAL_TO_DINT(dut_sensor_datas_3.data_lreal*100))/100;
//sensor_4
dut_sensor_datas_4.data_byte[3]:=Sensor_Reading_4[0];
dut_sensor_datas_4.data_byte[2]:=Sensor_Reading_4[1];
dut_sensor_datas_4.data_byte[1]:=Sensor_Reading_4[2];
dut_sensor_datas_4.data_byte[0]:=Sensor_Reading_4[3];
TemperatureSensors_4:=DINT_TO_LREAL(LREAL_TO_DINT(dut_sensor_datas_4.data_lreal*100))/100;
//sensor_5
dut_sensor_datas_5.data_byte[3]:=Sensor_Reading_5[0];
dut_sensor_datas_5.data_byte[2]:=Sensor_Reading_5[1];
dut_sensor_datas_5.data_byte[1]:=Sensor_Reading_5[2];
dut_sensor_datas_5.data_byte[0]:=Sensor_Reading_5[3];
TemperatureSensors_5:=DINT_TO_LREAL(LREAL_TO_DINT(dut_sensor_datas_5.data_lreal*100))/100;
//sensor_6
dut_sensor_datas_6.data_byte[3]:=Sensor_Reading_6[0];
dut_sensor_datas_6.data_byte[2]:=Sensor_Reading_6[1];
dut_sensor_datas_6.data_byte[1]:=Sensor_Reading_6[2];
dut_sensor_datas_6.data_byte[0]:=Sensor_Reading_6[3];
TemperatureSensors_6:=DINT_TO_LREAL(LREAL_TO_DINT(dut_sensor_datas_6.data_lreal*100))/100;
//sensor_7
dut_sensor_datas_7.data_byte[3]:=Sensor_Reading_7[0];
dut_sensor_datas_7.data_byte[2]:=Sensor_Reading_7[1];
dut_sensor_datas_7.data_byte[1]:=Sensor_Reading_7[2];
dut_sensor_datas_7.data_byte[0]:=Sensor_Reading_7[3];
TemperatureSensors_7:=DINT_TO_LREAL(LREAL_TO_DINT(dut_sensor_datas_7.data_lreal*100))/100;
//sensor_8
dut_sensor_datas_8.data_byte[3]:=Sensor_Reading_8[0];
dut_sensor_datas_8.data_byte[2]:=Sensor_Reading_8[1];
dut_sensor_datas_8.data_byte[1]:=Sensor_Reading_8[2];
dut_sensor_datas_8.data_byte[0]:=Sensor_Reading_8[3];
TemperatureSensors_8:=DINT_TO_LREAL(LREAL_TO_DINT(dut_sensor_datas_8.data_lreal*100))/100;
//计算temperature_gradient
temperature_gradient_2_To_1:=ABS(TemperatureSensors_2-TemperatureSensors_1);
temperature_gradient_3_To_2:=ABS(TemperatureSensors_3-TemperatureSensors_2);
temperature_gradient_5_To_4:=ABS(TemperatureSensors_5-TemperatureSensors_4);
temperature_gradient_6_To_5:=ABS(TemperatureSensors_6-TemperatureSensors_5);
temperature_gradient_max:=MAX(temperature_gradient_2_To_1,temperature_gradient_3_To_2,
							temperature_gradient_5_To_4,temperature_gradient_6_To_5);
//计算大梁上的温度平均值
temperature_average:=DINT_TO_LREAL(LREAL_TO_DINT((TemperatureSensors_1+TemperatureSensors_2+TemperatureSensors_3+TemperatureSensors_4
					+TemperatureSensors_5+TemperatureSensors_6)/6*100))/100;
					
					
					
					
					
					
					]]></ST>
