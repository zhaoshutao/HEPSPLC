	LED_Error:=TRUE;
END_IF
IF ABS(PosDiff_RightBottomLin_actual)>PosDiff_RightBottomLin THEN
	ErrorID:=204;
	ErrorDescription:='ABS(PosDiff_RightBottomLin_actual)>PosDiff_RightBottomLin';
	Error:=TRUE;
	FB_PlcEmergencyStop();
	LED_Error:=TRUE;
END_IF
