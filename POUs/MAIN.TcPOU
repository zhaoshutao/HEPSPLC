    <Action Name="A_SetLinearEncoderPositionBias" Id="{6f3d3f8e-255d-4e09-853f-5fdf91122229}">
      <Implementation>
        <ST><![CDATA[A_SetLinearEncoderPositionBiasMC();
PositionBiasTON(PT:=T#0.1S);
CASE state_setLEPosBias OF
	0:
		PositionBiasTON.IN:=TRUE;
		IF MC_SetLEPosBias_LeftTop.Error OR MC_SetLEPosBias_RightTop.Error OR 
			MC_SetLEPosBias_LeftBottom.Error OR MC_SetLEPosBias_RightBottom.Error THEN
			state_setLEPosBias:=-100;
		ELSE 
			state_setLEPosBias:=1;
		END_IF
	1:
		IF NOT MC_SetLEPosBias_LeftTop.Done OR NOT MC_SetLEPosBias_RightTop.Done OR 
			NOT MC_SetLEPosBias_LeftBottom.Done OR NOT MC_SetLEPosBias_RightBottom.Done THEN
			MC_SetLEPosBias_LeftTop.Execute:=TRUE;
			MC_SetLEPosBias_RightTop.Execute:=TRUE;
			MC_SetLEPosBias_LeftBottom.Execute:=TRUE;
			MC_SetLEPosBias_RightBottom.Execute:=TRUE;
			state_setLEPosBias:=2;
		ELSE
			state_setLEPosBias:=-100;
		END_IF
	2:
		IF MC_SetLEPosBias_LeftTop.Done OR NOT MC_SetLEPosBias_RightTop.Done OR 
			NOT MC_SetLEPosBias_LeftBottom.Done OR NOT MC_SetLEPosBias_RightBottom.Done THEN
			IF NOT PositionBiasTON.Q THEN
				state_setLEPosBias:=2;
			ELSE
				state_setLEPosBias:=-100;
			END_IF
		END_IF
		IF  MC_SetLEPosBias_LeftTop.Done AND MC_SetLEPosBias_RightTop.Done AND
			MC_SetLEPosBias_LeftBottom.Done AND MC_SetLEPosBias_RightBottom.Done THEN
			state_setLEPosBias:=88;
		END_IF
	88:
		PositionBiasTON.IN:=FALSE;
		PositionBiasDone:=TRUE;
	-100:
		PositionBiasTON.IN:=FALSE;
		PositionBiasDone:=FALSE;
		Error:=TRUE;
		ErrorId:=901;
		ErrorDescription:='Set LinearEncoderPositionBias Error';
		Plc_EmergencyStop:=FALSE;
END_CASE]]></ST>
      </Implementation>
    </Action>
