	RightBottomLinBusy:BOOL;
	RightBottomLin_m_state: DINT;
	RightBottomLin_s_state: DINT;


	btn_LeftTopStop: BOOL;
	(*¼ì²âµ½ÏÞÎ»£¬»Ö¸´Move×´Ì¬*)
